# 环境变量配置示例
# 复制此文件为 .env.local 并根据需要修改配置

# 应用基础配置
VITE_APP_TITLE=Frontend Template
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=基于 Vue 3 + TypeScript + Vite 的现代前端开发模板

# API 配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# 应用配置
VITE_APP_PORT=5173
VITE_APP_OPEN_BROWSER=true

# 开发配置
VITE_APP_ENV=development
VITE_APP_DEBUG=true
VITE_APP_MOCK=true
VITE_APP_CONSOLE_LOG=true